    <entity name="ProductLabelProdCatTabUploadImgDefault" type="product_label2"
            extends="ProductLabelProdCatTabTextOnlyDefault">
        <data key="LabelTypeSelect">Upload Image</data>
        <data key="LabelFileName">1.jpg</data>
