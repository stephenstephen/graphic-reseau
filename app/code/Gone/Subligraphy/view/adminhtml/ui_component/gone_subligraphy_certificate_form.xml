<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">gone_subligraphy_certificate_form.certificate_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Gone\Subligraphy\Block\Adminhtml\Certificate\Edit\BackButton"/>
			<button name="delete" class="Gone\Subligraphy\Block\Adminhtml\Certificate\Edit\DeleteButton"/>
			<button name="save" class="Gone\Subligraphy\Block\Adminhtml\Certificate\Edit\SaveButton"/>
			<button name="save_and_continue" class="Gone\Subligraphy\Block\Adminhtml\Certificate\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>gone_subligraphy_certificate_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>gone_subligraphy_certificate_form.certificate_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="certificate_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="certificate_form_data_source" class="Gone\Subligraphy\Model\Certificate\DataProvider">
			<settings>
				<requestFieldName>certificate_id</requestFieldName>
				<primaryFieldName>certificate_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="customer_id" formElement="input" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Certificate</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">customer_id</label>
				<dataScope>customer_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="image" formElement="input" sortOrder="20">
			<argument name="data" xsi:type="array">
