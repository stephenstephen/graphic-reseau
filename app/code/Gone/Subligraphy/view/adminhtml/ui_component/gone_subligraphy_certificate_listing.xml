<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">gone_subligraphy_certificate_listing.gone_subligraphy_certificate_listing_data_source</item>
		</item>
	</argument>

	<settings>
		<spinner>gone_subligraphy_certificate_columns</spinner>
		<deps>
			<dep>gone_subligraphy_certificate_listing.gone_subligraphy_certificate_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource name="gone_subligraphy_certificate_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">certificate_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Gone_Subligraphy::Certificate</aclResource>
		<dataProvider name="gone_subligraphy_certificate_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>certificate_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="gone_subligraphy_certificate_columns">
        <settings>
            <editorConfig>
                <param name="selectProvider" xsi:type="string">gone_subligraphy_certificate_listing.gone_subligraphy_certificate_listing.gone_subligraphy_certificate_columns.ids</param>
                <param name="enabled" xsi:type="boolean">false</param>
                <param name="indexField" xsi:type="string">certificate_id</param>
                <param name="clientConfig" xsi:type="array">
                    <item name="saveUrl" xsi:type="url" path="gone_subligraphy/Certificate/inlineEdit"/>
                    <item name="validateBeforeSave" xsi:type="boolean">false</item>
                </param>
            </editorConfig>
            <childDefaults>
                <param name="fieldAction" xsi:type="array">
                    <item name="provider" xsi:type="string">gone_subligraphy_certificate_listing.gone_subligraphy_certificate_listing.gone_subligraphy_certificate_columns_editor</item>
                    <item name="target" xsi:type="string">startEdit</item>
                    <item name="params" xsi:type="array">
                        <item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
                        <item name="1" xsi:type="boolean">true</item>
                    </item>
                </param>
            </childDefaults>
        </settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>certificate_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="certificate_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="customer">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">customer</item>
                    <item name="sortOrder" xsi:type="number">10</item>
                </item>
            </argument>
		</column>
		<column name="title">
			<settings>
				<filter>text</filter>
				<label translate="true">title</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="year">
			<settings>
				<filter>text</filter>
				<label translate="true">year</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="width">
			<settings>
				<filter>text</filter>
				<label translate="true">width</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="height">
			<settings>
				<filter>text</filter>
				<label translate="true">height</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="author">
			<settings>
				<filter>text</filter>
				<label translate="true">author</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="manufacturer">
			<settings>
				<filter>text</filter>
				<label translate="true">manufacturer</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="publisher">
			<settings>
				<filter>text</filter>
				<label translate="true">publisher</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created_at">
			<settings>
				<filter>text</filter>
				<label translate="true">created_at</label>
				<editor>
					<editorType>date</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="number">
			<settings>
				<filter>text</filter>
				<label translate="true">number</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="count">
			<settings>
				<filter>text</filter>
				<label translate="true">count</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>

        <actionsColumn name="actions" class="Gone\Subligraphy\Ui\Component\Listing\Column\CertificateActions" sortOrder="20">
            <settings>
                <indexField>certificate_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
            </settings>
        </actionsColumn>
	</columns>
</listing>
