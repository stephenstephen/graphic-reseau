<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Create new Subligraphy certificate</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <arguments>
                <argument translate="true" name="title" xsi:type="string">Create new Subligraphy certificate</argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Gone\Subligraphy\Block\FormCertificates" name="graphicreseau_list_certificates" template="Gone_Subligraphy::form_certificates.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
