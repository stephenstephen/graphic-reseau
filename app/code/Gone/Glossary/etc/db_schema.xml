		<column xsi:type="smallint" name="definition_id" padding="6" unsigned="true" nullable="false" identity="true"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="definition_id"/>
		</constraint>
		<column name="status" nullable="false" xsi:type="boolean"/>
		<column name="text" nullable="false" xsi:type="varchar"/>
		<column name="case_sensible" nullable="false" xsi:type="boolean"/>
		<column name="description" nullable="false" xsi:type="text"/>
	</table>
</schema>
